{
  "name": "great-electron",
  "version": "0.0.1",
  "description": "桌面客户端（Windows && Mac）SDK",
  "author": "GreatFED",
  "license": "MIT",
  "main": "dist/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/taurusdigitaldesign/great-electron.git"
  },
  "scripts": {
    "start": "electron .",
    "build:native": "set GOOS=js&&set GOARCH=wasm&&go build -o ./dist/render/lib.wasm ./src/native/lib.go",
    "build:main:tsc": "tsc",
    "build:main": "cross-env NODE_ENV=development node ./scripts/build.js",
    "release:channel": "node ./scripts/publish.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "cross-env": "^7.0.2",
    "electron": "^11.0.3",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "eslint": "^7.3.1",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@bit/greatfed.quickfox.ipc": "^0.1.6",
    "axios": "^0.19.2",
    "electron-log": "^4.3.0",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.1",
    "ffi-napi": "^3.1.0",
    "ping": "^0.3.0"
  }
}
